{
  "name": "ainconv",
  "private": false,
  "version": "0.2.1",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "build:watch": "tsc -w",
    "prepublishOnly": "npm run build",
    "check": "biome check",
    "check:fix": "biome check --write"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@rollup/plugin-typescript": "^12.1.1",
    "bun-types": "^1.1.33",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.8.1",
    "typescript": "^5.6.3",
    "vite": "^5.4.10"
  },
  "description": "Ainu writing system converter.",
  "author": "mkpoli <mkpoli@mkpo.li>",
  "license": "MIT",
  "homepage": "https://github.com/mkpoli/ainconv",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mkpoli/ainconv.git"
  },
  "bugs": {
    "url": "https://github.com/mkpoli/ainconv/issues"
  },
  "keywords": [
    "ainu",
    "writing system",
    "language",
    "nlp",
    "converter"
  ],
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "commonjs": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "browser": "dist/index.umd.cjs",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "umd": "./dist/index.umd.js",
      "browser": "./dist/index.umd.js",
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./convert": "./dist/convert.d.ts"
  }
}
